<Window x:Class="Calculator_jyw_.History"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculator_jyw_"
        mc:Ignorable="d"
        Title="History" 
        Height="400" Width="300">
    
    <Window.DataContext>
        <local:CalculatorViewModel/>
    </Window.DataContext>
    
    <Grid  Height="300" VerticalAlignment="Bottom">
        <ListBox ItemsSource="{Binding ResultList}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding ResultList}" />
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>

        <!--<ItemsControl ItemsSource="{Binding ResultList}">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding}" />
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>-->
    </Grid>
</Window>
